package com.bridgeit.functionandlibraries;

import utility.Utility2;

public class StringFunctionC {
public static void main(String[] args) {
	System.out.println("Enter string: ");
	String str = Utility2.getString();
	int l = str.length();
	Utility2.permutation(str,0,l-1);
	//System.out.println(str);
}
}
