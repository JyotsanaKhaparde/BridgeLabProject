package com.bridgelabz.datastructure;

public class MyOrderedList {
	
    Node<T> head;
    public void add(T data){
   
		Node<T> node = new Node<T>( data,null);

        if (head == null)
            head = node;

        else {
            traverse = self.head;
            if (self.head.data > node.data){
                self.head = node;
                node.next = traverse;
                }

            if (self.head.data < node.data){
                temp = self.head;
                while (traverse.next != None){
                    if( traverse.data < node.data) {
                        temp = traverse;
                    traverse = traverse.next;
                    }

                if (traverse.data < node.data)
                    temp = traverse;

                temp1 = temp.next;
                temp.next = node;
                node.next = temp1;
            }
            }
        }
}
